# Environment Configuration for Employee Management System

# API Base URL - Change this to your backend server URL
REACT_APP_API_URL=http://localhost:8080/api

# Demo API URL - For demo data endpoints
REACT_APP_DEMO_API_URL=http://localhost:8080/api/demo

# Environment Mode
REACT_APP_ENV=development

# Enable/Disable Demo Mode Fallback
REACT_APP_ENABLE_DEMO_FALLBACK=true

# # API Timeout (milliseconds)
# REACT_APP_API_TIMEOUT=10000000

# App Version
REACT_APP_VERSION=1.0.0

# Company Information
REACT_APP_COMPANY_NAME=Your Company Name
REACT_APP_COMPANY_LOGO=/logo.png
