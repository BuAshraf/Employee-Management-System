{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "es6"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "downlevelIteration": true,
        "baseUrl": "src",
        "paths": {
            "@/*": [
                "*"
            ],
            "@/components/*": [
                "components/*"
            ],
            "@/services/*": [
                "services/*"
            ],
            "@/utils/*": [
                "utils/*"
            ],
            "@/context/*": [
                "context/*"
            ],
            "@/pages/*": [
                "components/pages/*"
            ]
        }
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}